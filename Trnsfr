Sub Macro3()
'
' Macro3 Macro
'

'
    Sheets("SSI").Select
    Columns("A:ER").Select
    Selection.Copy
    Windows("Dashboard Front End.xlsx").Activate
    Sheets("SSI").Select
    Range("A1").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Windows("Dashboard Front End.xlsx").Activate

End Sub
Sub Macro5()
'
' Macro5 Macro
'

'
    Windows("Dashboard Front End.xlsx").Activate
    Sheets("SSI").Select
    Columns("D:D").Select
    Selection.Delete Shift:=xlToLeft
    Columns("X:X").Select
    Selection.Delete Shift:=xlToLeft
    ActiveWindow.SmallScroll ToRight:=23
    Columns("AR:AR").Select
    Selection.Delete Shift:=xlToLeft
    ActiveWindow.SmallScroll ToRight:=20
    Columns("BL:BL").Select
    Selection.Delete Shift:=xlToLeft
    ActiveWindow.SmallScroll ToRight:=22
    Columns("CF:CF").Select
    Selection.Delete Shift:=xlToLeft
    ActiveWindow.SmallScroll ToRight:=24
    Columns("CZ:CZ").Select
    Selection.Delete Shift:=xlToLeft
    ActiveWindow.SmallScroll ToRight:=19
    Columns("DT:DT").Select
    Selection.Delete Shift:=xlToLeft
    ActiveWindow.SmallScroll ToRight:=13
    Windows("Dashboard.xlsm").Activate
End Sub
