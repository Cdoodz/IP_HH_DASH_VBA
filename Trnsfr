Sub TransferToFrontEnd()
'
' 9J CCU
'

'
    Sheets("CCU").Select
    ActiveWindow.ScrollColumn = 1
    Columns("A:R").Select
    ActiveWindow.SmallScroll ToRight:=6
    Range("A:R,U:W").Select
    Range("U1").Activate
    Selection.Copy
    Windows("Dashboard Front End.xlsx").Activate
    Sheets("9J CCU").Select
    ActiveWindow.ScrollColumn = 2
    ActiveWindow.ScrollColumn = 1
    Range("A1").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Windows("Dashboard V2.xlsm").Activate
'
' CP9 Prog Care
'

'
    Sheets("ProCare").Select
    ActiveWindow.ScrollColumn = 1
